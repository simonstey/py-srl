@prefix mf: <http://www.w3.org/2001/sw/DataAccess/tests/test-manifest#> .
@prefix srt: <http://www.w3.org/ns/shacl-rules-test#> .
@prefix pyrl: <http://example.org/py-srl/test#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix : <#> .

<> a mf:Manifest ;
    rdfs:label "Transitive Relationship Tests" ;
    rdfs:comment "Tests for transitive closure computation (ancestor, part-of)" ;
    mf:entries (
        :transitive-001
        :transitive-002
    ) .

:transitive-001 a srt:RulesEvalTest ;
    mf:name "ancestor" ;
    rdfs:comment "Compute ancestor relationships transitively" ;
    mf:action [
        srt:ruleset <transitive-001.srl> ;
        srt:data <transitive-001.ttl> ;
    ] ;
    mf:result <transitive-001-result.ttl> ;
    mf:status srt:approved .

:transitive-002 a srt:RulesEvalTest ;
    mf:name "part-of" ;
    rdfs:comment "Transitive part-whole relationships" ;
    mf:action [
        srt:ruleset <transitive-002.srl> ;
        srt:data <transitive-002.ttl> ;
    ] ;
    mf:result <transitive-002-result.ttl> ;
    mf:status srt:approved .
